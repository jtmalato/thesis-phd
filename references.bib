	number = {8},
	urldate = {2024-02-09},
	journal = {Journal of Clinical Pathology},
	author = {Smith, J. and Fritz, E. L. and Kerr, J. R. and Cleare, A. J. and Wessely, S. and Mattey, D. L.},
