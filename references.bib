@article{lustig2023ModellingImpact,
	title = {Modelling the impact of the {Omicron} {BA}.5 subvariant in {New} {Zealand}},
	volume = {20},
	url = {https://royalsocietypublishing.org/doi/10.1098/rsif.2022.0698},
