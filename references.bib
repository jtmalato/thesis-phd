	pages = {486--495},
	file = {Accepted Version:/Users/jmalato-admin/Zotero/storage/6ZAHXJNJ/Mustelin et al. - 2019 - The Contribution of PTPN22 to Rheumatic Disease.pdf:application/pdf},
}

@article{moore2013DNAMethylation,
	title = {{DNA} {Methylation} and {Its} {Basic} {Function}},
	volume = {38},
