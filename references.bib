	keywords = {diagnostic protocol, Fatigue Syndrome, Chronic, Humans, case definition, Myalgic encephalomyelitis/chronic fatigue syndrome, Diagnosis, Differential, Canada, case definitions, chronic illness, Cognitive Dysfunction, Consensus, diagnostic criteria, factor analysis, Pain, United States},
	pages = {840--847},
