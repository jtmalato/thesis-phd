	keywords = {Humans, Adult, Female, Male, Middle Aged, Case-Control Studies, Leukocytes, Mononuclear, Oxidative Stress, Gene Expression Profiling, Oligonucleotide Array Sequence Analysis, Diabetes Mellitus, Type 2, Nucleic Acid Hybridization},
