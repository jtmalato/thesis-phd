	journal = {Clinical and Experimental Immunology},
	author = {Rutkowska-Zapa\l{}a, M and Suski, M and Szatanek, R and Lenart, M and Węglarczyk, K and Olszanecki, R and Grodzicki, T and Strach, M and G\k{a}sowski, J and Siedlar, M},
