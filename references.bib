	author = {Moore, Lisa D. and Le, Thuc and Fan, Guoping},
	month = jan,
	year = {2013},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Epigenetics in the nervous system, Target identification},
	pages = {23--38},
	file = {Full Text PDF:/Users/jmalato-admin/Zotero/storage/TJ7MA2WA/Moore et al. - 2013 - DNA Methylation and Its Basic Function.pdf:application/pdf},
}

