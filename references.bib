	keywords = {Fatigue Syndrome, Chronic, Humans, epidemiology, prevalence, Adult, Depression, Female, Male, Anxiety, Middle Aged, Surveys and Questionnaires, Fatigue, Quality of Life, Cohort Studies, Chronic Disease, Prevalence, Aged, chronic fatigue, Cross-Sectional Studies, Poland, quality of life},
