	journal = {Blood},
	author = {Goodier, Martin R. and White, Matthew J. and Darboe, Alansana and Nielsen, Carolyn M. and Goncalves, Adriana and Bottomley, Christian and Moore, Sophie E. and Riley, Eleanor M.},
