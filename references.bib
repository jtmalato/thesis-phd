@article{hatziagelaki2018MyalgicEncephalomyelitis,
	title = {Myalgic {Encephalomyelitis}/{Chronic} {Fatigue} {Syndrome}—{Metabolic} {Disease} or {Disturbed} {Homeostasis} due to {Focal} {Inflammation} in the {Hypothalamus}?},
	volume = {367},
	copyright = {Copyright © 2018 by The American Society for Pharmacology and Experimental Therapeutics},
