	keywords = {Fatigue Syndrome, Chronic, Humans, Adult, Depression, Female, Male, Middle Aged, Adolescent, Leukocyte Count, Biomarkers, Fatigue, Young Adult, Inflammation, C-Reactive Protein, Sex Factors, Age Factors, Body Mass Index, Linear Models, Psychiatric Status Rating Scales, Neuropsychological Tests},
