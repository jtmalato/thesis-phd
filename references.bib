	pmid = {18298808},
	pmcid = {PMC2375137},
	keywords = {Reproducibility of Results, Gene Expression Profiling, Oligonucleotide Array Sequence Analysis, Statistics as Topic, Algorithms, Artifacts, Sensitivity and Specificity},
	pages = {114},
	file = {Full Text:/Users/jmalato-admin/Zotero/storage/PX8KCSJV/Kim and van de Wiel - 2008 - Effects of dependence in high-dimensional multiple.pdf:application/pdf},
}

