	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/21641846.2022.2062169},
	keywords = {chronic fatigue syndrome, Myalgic encephalomyelitis, economic impact, disease burden, funding},
	pages = {83--93},
}

