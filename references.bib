	keywords = {Humans, COVID-19, Adult, Female, Male, Middle Aged, Antibodies, Viral, Time Factors, Immunoglobulin G, Cohort Studies, Aged, Antibodies, Neutralizing, Sex Factors, Age Factors, BNT162 Vaccine, Body Mass Index, Health Personnel, Immunization, Secondary, Immunocompromised Host, Immunogenicity, Vaccine, Israel, Linear Models, Vaccine Efficacy},
