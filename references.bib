	urldate = {2023-12-28},
	journal = {Nature Medicine},
	author = {Lau, Jonathan J. and Cheng, Samuel M. S. and Leung, Kathy and Lee, Cheuk Kwong and Hachim, Asmaa and Tsang, Leo C. H. and Yam, Kenny W. H. and Chaothai, Sara and Kwan, Kelvin K. H. and Chai, Zacary Y. H. and Lo, Tiffany H. K. and Mori, Masashi and Wu, Chao and Valkenburg, Sophie A. and Amarasinghe, Gaya K. and Lau, Eric H. Y. and Hui, David S. C. and Leung, Gabriel M. and Peiris, Malik and Wu, Joseph T.},
