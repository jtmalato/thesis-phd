	keywords = {ME/CFS, biobank, partnership, patient engagement PE, research infrastructure},
	pages = {1026},
	file = {Full Text:/Users/jmalato-admin/Zotero/storage/RVV4CYJN/Lacerda et al. - 2018 - The UK MECFS Biobank A Disease-Specific Biobank .pdf:application/pdf},
}

