	number = {46},
	journal = {Vaccine},
	author = {Magnus, Per and Gunnes, Nina and Tveito, Kari and Bakken, Inger Johanne and Ghaderi, Sara and Stoltenberg, Camilla and Hornig, Mady and Lipkin, W. Ian and Trogstad, Lill and H\r{a}berg, Siri E.},
