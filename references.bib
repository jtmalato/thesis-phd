	pmid = {19469714},
	keywords = {Fatigue Syndrome, Chronic, Humans, Adult, Female, Male, Middle Aged, Adolescent, Physical Exertion, Cohort Studies, Young Adult, Blood Pressure, Blood Volume, Cardiac Output, Exercise Test, Myocardial Contraction, Sedentary Behavior, Severity of Illness Index},
	pages = {125--135},
	file = {Hurwitz et al. - 2009 - Chronic fatigue syndrome illness severity, sedent.pdf:/Users/jmalato-admin/Zotero/storage/K76NQM59/Hurwitz et al. - 2009 - Chronic fatigue syndrome illness severity, sedent.pdf:application/pdf},
