@article{dedeurwaerder2014ComprehensiveOverview,
	title = {A comprehensive overview of {Infinium} {HumanMethylation450} data processing},
	volume = {15},
	issn = {1477-4054},
	doi = {10.1093/bib/bbt054},
	abstract = {Infinium HumanMethylation450 beadarray is a popular technology to explore DNA methylomes in health and disease, and there is a current explosion in the use of this technique. Despite experience acquired from gene expression microarrays, analyzing Infinium Methylation arrays appeared more complex than initially thought and several difficulties have been encountered, as those arrays display specific features that need to be taken into consideration during data processing. Here, we review several issues that have been highlighted by the scientific community, and we present an overview of the general data processing scheme and an evaluation of the different normalization methods available to date to guide the 450K users in their analysis and data interpretation.},
